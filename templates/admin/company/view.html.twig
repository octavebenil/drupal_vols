<div class="title">
  <span style="float: right;">
    <a class="btn btn-primary" href="{{ path('vols.company_list') }}">Retour</a>
  </span>
</div>

<div class="table-responsive">
  <table class="table table-hover">
    <tbody>
        <tr>
          <td>ID</td><td> : </td><td>{{ company.id }}</td>
        </tr>
        <tr>
          <td>CODE</td><td> : </td><td>{{ company.code }}</td>
        </tr>

        <tr>
          <td>NOM</td><td> : </td><td>{{ company.name }}</td>
        </tr>

        <tr>
          <td>Ajouter le</td><td> : </td><td>{{ company.created_at|date('d/m/Y H:i:s') }}</td>
        </tr>


        <tr>
          <td>Modifier le</td><td> : </td><td>{{ company.updated_at|date('d/m/Y H:i:s') }}</td>
        </tr>

    </tbody>
  </table>
</div>
